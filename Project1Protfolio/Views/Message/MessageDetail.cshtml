@model Project1Protfolio.Models.Message
@{
    ViewBag.Title = "MessageDetail";
    Layout = "~/Views/Admin/Index.cshtml";
}

<h2 class="mb-4">Message Detayları Sayfası</h2>

<b>Gönderen Adı Soyadı : </b>@Html.DisplayFor(x => x.Name)
<br /><br />
      <b>Gönderen Email: </b> @Html.DisplayFor(x=>x.Email)
<br /><br />
      <b>Gönderen Telefon Numarası: </b> @Html.DisplayFor(x=>x.PhoneNumber)
<br /><br />
      <b>Gönderen Mesaj Konusu: </b> @Html.DisplayFor(x=>x.Subject)
<br /><br />
      <b>Gönderen Mesaj Detayları: </b> @Html.DisplayFor(x=>x.MessageDetail)
<br /><br />
      <b>Tarih: </b> @Html.DisplayFor(x=>x.SendDate)
<br /><br />


